<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="TableControls" blendMode="COLOR_BURN" onMouseClicked="#anchorPaneClicked" prefHeight="504.0" prefWidth="844.0" stylesheets="@../Stylesheets/Stylesheet.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ApplicationPage">
    <children>
      <AnchorPane fx:id="dragAnchor" layoutX="-1.0" prefHeight="25.0" prefWidth="737.0" />
        <TextField fx:id="searchText" layoutX="149.0" layoutY="30.0" promptText="Search">
            <tooltip>
                <Tooltip text="Search" />
            </tooltip>
        </TextField>
        <Hyperlink fx:id="AppClose" layoutX="867.0" layoutY="2.0" onAction="#close" text="x" />
        <Hyperlink fx:id="AppMinimize" layoutX="847.0" layoutY="2.0" onAction="#minimize" prefHeight="23.0" prefWidth="14.0" text="-" />
        <AnchorPane fx:id="SideMenu" prefHeight="504.0" prefWidth="149.0">
            <children>
                <Hyperlink layoutX="50.0" layoutY="438.0" prefHeight="23.0" prefWidth="24.0" text="SV" />
                <Label layoutX="74.0" layoutY="439.0" prefHeight="17.0" prefWidth="5.0" text="|" textFill="WHITE" />
                <Hyperlink layoutX="78.0" layoutY="438.0" prefHeight="23.0" prefWidth="24.0" text="EN" />
                <ImageView fx:id="profilePic" fitHeight="139.0" fitWidth="135.0" layoutX="6.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/logo.png" />
                    </image>
                </ImageView>
                <Hyperlink fx:id="logOut" layoutX="48.0" layoutY="460.0" onAction="#logOut" text="Log Out" />
            <Button id="CSVBtn" fx:id="importCSVBtn" layoutX="7.0" layoutY="179.0" mnemonicParsing="false" onAction="#CSVImport" prefHeight="30.0" prefWidth="135.0" text="Import CSV" />
            <ImageView blendMode="DARKEN" fitHeight="16.0" fitWidth="17.0" layoutX="11.0" layoutY="188.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/import.png" />
               </image>
            </ImageView>
            <Button fx:id="accountSettingsBtn" layoutX="6.0" layoutY="222.0" mnemonicParsing="false" onAction="#accountSettings" prefHeight="30.0" prefWidth="135.0" text="Account Settings">
               <graphic>
                  <ImageView blendMode="DARKEN" fitHeight="22.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/settings.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button id="AddShelfBtn" fx:id="AddShelfBtn" layoutX="6.0" layoutY="263.0" mnemonicParsing="false" onAction="#addShelf" prefHeight="25.0" prefWidth="135.0" text="Add a shelf" />
            </children>
        </AnchorPane>
        <Hyperlink fx:id="addBtn" layoutX="759.0" layoutY="28.0" onAction="#add" prefHeight="23.0" prefWidth="24.0" text="+">
            <tooltip>
                <Tooltip text="Add Product" />
            </tooltip>
            <font>
                <Font size="15.0" />
            </font>
        </Hyperlink>
        <Hyperlink fx:id="removeBtn" layoutX="736.0" layoutY="28.0" onAction="#remove" text="-">
            <font>
                <Font size="15.0" />
            </font>
            <tooltip>
                <Tooltip text="Remove Product" />
            </tooltip>
        </Hyperlink>
        <ComboBox fx:id="tableBox" layoutX="783.0" layoutY="30.0" onAction="#tableSelection" prefHeight="25.0" prefWidth="98.0" />
      <Button fx:id="editBtn" layoutX="700.0" layoutY="29.0" mnemonicParsing="false" onAction="#update" prefHeight="23.0" prefWidth="34.0">
         <graphic>
            <ImageView blendMode="DARKEN" fitHeight="23.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/edit.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
        <TableView fx:id="infoTable" layoutX="149.0" layoutY="58.0" prefHeight="446.0" prefWidth="737.0">
            <columns>
                <TableColumn fx:id="colID" prefWidth="75.0" text="ProductID" />
                <TableColumn fx:id="colName" prefWidth="75.0" text="Name" />
                <TableColumn fx:id="colStock" prefWidth="75.0" text="Quantity" />
                <TableColumn fx:id="colPrice" prefWidth="75.0" text="Price" />
            <TableColumn fx:id="colCategory" prefWidth="75.0" text="Category" />
            <TableColumn fx:id="colShelf" prefWidth="75.0" text="Shelf" />
            <TableColumn fx:id="colSupplier" prefWidth="75.0" text="Supplier" />
            <TableColumn fx:id="colSupplierID" prefWidth="75.0" text="SupplierID" />
            <TableColumn fx:id="colCost" prefWidth="75.0" text="Cost" />
            </columns>
        </TableView>
      <TableView fx:id="supplierTable" layoutX="149.0" layoutY="58.0" prefHeight="446.0" prefWidth="737.0">
        <columns>
          <TableColumn fx:id="supNameCol" prefWidth="114.66665649414062" text="Name" />
          <TableColumn fx:id="supPhoneCol" prefWidth="110.6666259765625" text="Phone" />
            <TableColumn fx:id="supStreetCol" minWidth="0.0" prefWidth="84.6666259765625" text="Street" />
            <TableColumn fx:id="supCityCol" minWidth="0.0" prefWidth="82.66668701171875" text="City" />
            <TableColumn fx:id="supCountryCol" minWidth="0.0" prefWidth="138.66668701171875" text="Country" />
            <TableColumn fx:id="supEmailCol" prefWidth="143.33333333333337" text="Email" />
        </columns>
      </TableView>
      <TableView fx:id="categoryTable" layoutX="149.0" layoutY="58.0" prefHeight="446.0" prefWidth="737.0">
        <columns>
            <TableColumn fx:id="catIDCol" prefWidth="156.00000762939453" text="Category ID" />
          <TableColumn fx:id="catNameCol" prefWidth="224.00003051757812" text="Category Name" />
        </columns>
      </TableView>
      <ImageView fitHeight="25.0" fitWidth="24.0" layoutX="149.0" layoutY="34.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/search.jpg" />
         </image>
      </ImageView>
    </children>
</AnchorPane>

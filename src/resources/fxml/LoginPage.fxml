<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="window" prefHeight="374.0" prefWidth="268.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.LoginPage">
   <children>
      <ImageView fitHeight="107.0" fitWidth="103.0" layoutX="83.0" layoutY="34.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/logo.png" />
         </image>
      </ImageView>
      <TextField fx:id="username" layoutX="52.0" layoutY="174.0" prefHeight="25.0" prefWidth="163.0" promptText="Username" />
      <PasswordField fx:id="password" layoutX="52.0" layoutY="211.0" prefHeight="25.0" prefWidth="163.0" promptText="Password" />
      <Hyperlink layoutX="95.0" layoutY="255.0" text="Forgot password?" textFill="#fff9f9">
         <font>
            <Font size="9.0" />
         </font></Hyperlink>
      <Button fx:id="loginBtn" layoutY="334.0" mnemonicParsing="false" onAction="#login" prefHeight="39.0" prefWidth="268.0" text="Log in" />
      <Hyperlink fx:id="register" layoutX="7.0" layoutY="2.0" onAction="#register" text="Create Account" />
      <Hyperlink fx:id="minimize" layoutX="216.0" layoutY="2.0" onAction="#minimize" prefHeight="23.0" prefWidth="20.0" text="-">
         <font>
            <Font size="15.0" />
         </font>
      </Hyperlink>
      <Hyperlink fx:id="close" layoutX="236.0" layoutY="5.0" onAction="#close" prefHeight="23.0" prefWidth="20.0" text="x" />
   </children>
</AnchorPane>

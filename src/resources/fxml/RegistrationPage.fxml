<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="368.0" prefWidth="278.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.RegistrationPage">
    <children>
        <Hyperlink fx:id="close" layoutX="258.0" layoutY="2.0" onAction="#close" text="x" />
        <Hyperlink fx:id="minimize" layoutX="244.0" layoutY="2.0" onAction="#minimize" text="-" />
        <TextField fx:id="username" layoutX="63.0" layoutY="116.0" promptText="Username" />
        <TextField fx:id="email" layoutX="63.0" layoutY="158.0" promptText="Email" />
        <TextField fx:id="phone" layoutX="63.0" layoutY="197.0" promptText="Phone" />
        <TextField fx:id="address" layoutX="63.0" layoutY="237.0" promptText="Address" />
        <Label layoutX="69.0" layoutY="52.0" text="Create an account" textFill="WHITE">
            <font>
                <Font size="17.0" />
            </font>
        </Label>
        <Hyperlink layoutX="7.0" layoutY="4.0" onAction="#cancel" text="Cancel" />
        <Button fx:id="registerBtn" layoutX="-1.0" layoutY="368.0" mnemonicParsing="false" onAction="#register" prefHeight="47.0" prefWidth="279.0" text="Register Account" />
      <PasswordField fx:id="password" layoutX="65.0" layoutY="279.0" promptText="Password" />
      <PasswordField fx:id="repeatedPW" layoutX="65.0" layoutY="320.0" promptText="Repeat Password" />
    </children>
</AnchorPane>

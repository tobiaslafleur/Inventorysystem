<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="390.0" prefWidth="278.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.AddSupplierPage">
    <children>
        <TextField fx:id="name" layoutX="65.0" layoutY="83.0" promptText="Name" />
        <TextField fx:id="phone" layoutX="65.0" layoutY="126.0" promptText="Phone" />
        <TextField fx:id="email" layoutX="63.0" layoutY="277.0" promptText="Email" />
        <Label layoutX="91.0" layoutY="42.0" text="Add Supplier" textFill="WHITE">
            <font>
                <Font size="17.0" />
            </font>
        </Label>
        <Hyperlink layoutX="224.0" layoutY="10.0" onAction="#cancel" text="Cancel" />
        <Button fx:id="addSupplierBtn" layoutX="-1.0" layoutY="350.0" mnemonicParsing="false" onAction="#addSupplier" prefHeight="40.0" prefWidth="279.0" text="Add supplier" />
      <TextField fx:id="street" layoutX="63.0" layoutY="163.0" promptText="Street" />
      <TextField fx:id="city" layoutX="63.0" layoutY="197.0" promptText="City" />
      <TextField fx:id="country" layoutX="63.0" layoutY="235.0" promptText="Country" />
    </children>
</AnchorPane>

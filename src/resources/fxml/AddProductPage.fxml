<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="462.0" prefWidth="277.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.AddProductPage">
   <children>
      <TextField fx:id="name" layoutX="65.0" layoutY="61.0" promptText="Product Name" />
      <TextField fx:id="stock" layoutX="65.0" layoutY="113.0" promptText="Stock" />
      <TextField fx:id="price" layoutX="66.0" layoutY="165.0" promptText="Price" />
      <TextField fx:id="cost" layoutX="65.0" layoutY="216.0" promptText="Cost" />
      <ComboBox fx:id="categories" layoutX="65.0" layoutY="267.0" prefWidth="150.0" promptText="Category" />
      <Hyperlink fx:id="addCategoryBtn" layoutX="215.0" layoutY="268.0" onAction="#add" text="+" />
      <ComboBox fx:id="shelves" layoutX="65.0" layoutY="314.0" prefWidth="150.0" promptText="Shelf" />
      <Hyperlink fx:id="addShelfBtn" layoutX="215.0" layoutY="315.0" onAction="#add" text="+" />
      <ComboBox fx:id="suppliers" layoutX="65.0" layoutY="362.0" prefWidth="150.0" promptText="Supplier" />
      <Hyperlink fx:id="addSupplierBtn" layoutX="215.0" layoutY="363.0" onAction="#add" text="+" />
      <Button fx:id="addProduct" layoutY="423.0" mnemonicParsing="false" onAction="#addProduct" prefHeight="40.0" prefWidth="279.0" text="Add product" />
      <Label fx:id="lblTitle" alignment="CENTER" layoutX="-1.0" layoutY="9.0" prefHeight="26.0" prefWidth="279.0" text="Add Product" textFill="WHITE">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="lblName" alignment="CENTER" layoutX="-2.0" layoutY="86.0" prefHeight="13.0" prefWidth="279.0" textFill="#dadada">
         <font>
            <Font size="9.0" />
         </font>
      </Label>
      <Label fx:id="lblStock" alignment="CENTER" layoutX="-2.0" layoutY="138.0" prefHeight="13.0" prefWidth="279.0" textFill="#dadada">
         <font>
            <Font size="9.0" />
         </font>
      </Label>
      <Label fx:id="lblPrice" alignment="CENTER" layoutY="190.0" prefHeight="13.0" prefWidth="279.0" textFill="#dadada">
         <font>
            <Font size="9.0" />
         </font>
      </Label>
      <Label fx:id="lblCost" alignment="CENTER" layoutY="241.0" prefHeight="13.0" prefWidth="279.0" textFill="#dadada">
         <font>
            <Font size="9.0" />
         </font>
      </Label>
      <Hyperlink fx:id="hyperCancel" layoutX="223.0" layoutY="10.0" onAction="#cancel" text="Cancel" />
   </children>
</AnchorPane>

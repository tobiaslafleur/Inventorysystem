<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.EditProductPage">
   <children>
      <Label layoutX="82.0" layoutY="32.0" text="Edit Product" textFill="WHITE">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Hyperlink fx:id="close" layoutX="229.0" layoutY="2.0" onAction="#close" text="x" />
      <Hyperlink fx:id="minimize" layoutX="215.0" layoutY="2.0" onAction="#minimize" text="-" />
      <Hyperlink fx:id="cancel" layoutX="8.0" layoutY="2.0" onAction="#cancel" text="Cancel" />
      <ComboBox fx:id="products" layoutX="50.0" layoutY="72.0" prefWidth="150.0" promptText="Product" />
      <TextField layoutX="51.0" layoutY="107.0" promptText="Name" />
      <TextField layoutX="51.0" layoutY="144.0" promptText="Quantity" />
      <TextField layoutX="51.0" layoutY="212.0" promptText="Price" />
      <TextField layoutX="51.0" layoutY="250.0" promptText="Shelf" />
      <TextField layoutX="51.0" layoutY="290.0" promptText="Cost" />
      <Button fx:id="updateBtn" layoutX="-1.0" layoutY="361.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="257.0" text="Update product" />
      <ComboBox fx:id="categories" layoutX="50.0" layoutY="176.0" prefWidth="150.0" promptText="Category" />
   </children>
</AnchorPane>
